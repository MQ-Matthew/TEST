                showClose : true,
                ignoreReadonly : true,
                widgetPositioning : {
                    horizontal : 'auto',
                    vertical : 'bottom'
                },
            });

        });

    }

    //删除一行
    function delete_goods(obj)
    {

        $(obj).parent().parent().remove();//删除一个物品对象
        tableDate();
    }

	//获取全日制教育Table数据
    function tableDate(){

    //全日制教育
        var tableJson = '{"data":[';
        $('#all_goods').find('tr').each(function(){
            var wu_number = $(this).find('td').eq(0).find('input').val();
            if(typeof(wu_number) != 'undefined'){
                tableJson += '{"jy_rxsj":"' + $(this).find('td').eq(0).find('input').val() + '",';
                tableJson += '"jy_bysj":"' + $(this).find('td').eq(1).find('input').val() + '",';
                tableJson += '"jy_xxmc":"' + $(this).find('td').eq(2).find('input').val() + '",';
                tableJson += '"jy_zy":"' + $(this).find('td').eq(3).find('input').val() + '",';
                tableJson += '"jy_xz":"' + $(this).find('td').eq(4).find('input').val() + '",';
                tableJson += '"jy_xl":"' + $(this).find('td').eq(5).find('select').val() + '",';
                tableJson += '"jy_xw":"' + $(this).find('td').eq(6).find('input').val() + '"},';
            }
        });
        tableJson = tableJson.substr(0, tableJson.length-1);//去掉最后一个，这个字段 豆号
        tableJson += "]}";

        if(tableJson =='{"data":]}'){
            tableJson='';
        }
        document.getElementById("jiaoyu").value = tableJson;
		
		}
		
