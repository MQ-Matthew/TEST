    function tbAddRow()
    {

        var html = '';
        html += '<tr>';

        html += '<td><input type="text" name="jy_rxsj[]" id="jy_rxsj" class="form-control input-date" value="" onchange="tableDate();" style="width:100%"> </td>';
        html += '<td><input type="text" name="jy_bysj[]" id="jy_bysj" class="form-control input-date" value="" onchange="tableDate();" style="width:100%"> </td>';
        html += '<td><input type="text" name="jy_xxmc[]" id="jy_xxmc"  value="" onblur="tableDate();" style="width:100%"> </td>';
        html += '<td><input type="text" name="jy_zy[]" id="jy_zy"  value="" onblur="tableDate();" style="width:100%"> </td>';
        html += '<td><input type="text" name="jy_xz[]" id="jy_xz"  value="" onblur="tableDate();" style="width:100%"> </td>';
        html += '<td>';
        html += ' <select class="form-control" id="jy_xl"  onchange="tableDate();" >';
        html += ' <option value="">请选择</option>';
        html += ' <option value="大专">大专</option>';
        html += ' <option value="本科">本科</option>';
        html += ' <option value="硕士">硕士</option>';
        html += ' <option value="博士">博士</option>';
        html += ' <option value="其它">其它</option>';
        html += '</select>';
        html += '</td>';

        html += '<td><input type="text" name="jy_xw[]" id="jy_xw"  value="" onblur="tableDate();" style="width:100%"> </td>';

        html += '<td><a href="javascript:void(0);" onclick="delete_goods(this)" class="delrow">删除</a></td>';
        html += '</tr>';
        /// alert(111111);
        $('#goods').before(html);
        /// $.parser.parse(jy_rxsj)

        $(document).ready(function() {

            $('.input-date').datetimepicker({
                format : 'yyyy-mm-dd',
                minView: "month",
                locale : 'zh-cn',
                //locale: moment.locale('zh-cn'),
                ignoreReadonly : true,
                autoclose: 1,
                widgetPositioning : {
                    horizontal : 'auto',
                    vertical : 'bottom'
                },
            });

            $(".input-daterange input").datetimepicker({
                format : "yyyy-mm-dd",
                locale : 'zh-cn',
                minView: "month",
                showTodayButton : true,
                showClose : true,
                ignoreReadonly : true,
                widgetPositioning : {
                    horizontal : 'auto',
                    vertical : 'bottom'
                },
            });

            $("#start_date").on("dp.change", function(e) {
                if ($("#end_date").length > 0) {
                    $('#end_date').data("DateTimePicker").minDate(e.date);
                }
            });

            $("#end_date").on("dp.change", function(e) {
                if ($("#start_date").length > 0) {
                    $('#start_date').data("DateTimePicker").maxDate(e.date);
                }
            });

