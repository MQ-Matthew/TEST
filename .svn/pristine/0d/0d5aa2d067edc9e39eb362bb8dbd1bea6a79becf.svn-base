<!--工作经历 table -->
<table cellspacing="0" class="table table-bordered table-condensed" style="width: 100%;">
    <thead>
    <th colspan="8">工作经历 ：</th>
    <tr>
        <th width="5%">序号</th>
        <th width="12%">起始时间</th>
        <th width="12%">结束时间</th>
        <th width="21%">工作单位</th>
        <th width="18%">工作部门</th>
        <th width="10%">工作职务</th>
        <th width="12%">备注</th>
        <th width="10%">操作</th>
    </tr>
    </thead>

    <empty name="info.gongzuo">
        <tbody id='all_gongzuos'>  <!--添加-->
        <tr>
            <td><input type="text" name="gongzuo_xuhao[]" id="gongzuo_xuhao"  value="" onblur="tableDate();" style="width:100%"> </td>

            <td><input type="text" name="gongzuo_qishishijian[]" id="gongzuo_qishishijian"  class="form-control input-date" value="" onblur="tableDate();" style="width:100%"> </td>
            <td><input type="text" name="gongzuo_jieshushijian[]" id="gongzuo_jieshushijian"  class="form-control input-date" value="" onblur="tableDate();" style="width:100%"> </td>

            <td><input type="text" name="gongzuo_danwei[]" id="gongzuo_danwei"  value="" onblur="tableDate();" style="width:100%"> </td>
            <td><input type="text" name="gongzuo_bumeng[]" id="gongzuo_bumeng"  value="" onblur="tableDate();" style="width:100%"></td>

            <td><input type="text" name="gongzuo_zhiwu[]" id="gongzuo_zhiwu"  value="" onblur="tableDate();" style="width:100%"></td>

            <td><input type="text" name="gongzuo_beizhu[]" id="gongzuo_beizhu"  value="" onblur="tableDate();" style="width:100%"></td>

            <td><a href="javascript:void(0);" onclick="delete_gongzuos(this)" class="delrow">删除</a></td>
        </tr>
        <tr id='gongzuos'></tr>
        </tbody>


        <else />

        <tbody id='all_gongzuos'> <!--编辑-->
        <tr id='gongzuos'></tr>
        </tbody>

    </empty>

    <tr>
        <td><button class="btn btn-small btn-success" type="button" onclick="gongzuo_tbAddRow()">添加工作经历</button></td>
        <td colspan="10"></td>
    </tr>

</table>
<input type="hidden" id="gongzuo" name="gongzuo" value="{$info.gongzuo}">
<!--工作经历 js -->
<script type="text/javascript">
    //添加一行
    function gongzuo_tbAddRow()
    {

        var gongzuo_html = '';
        gongzuo_html += '<tr>';

        gongzuo_html += '<td><input type="text" name="gongzuo_xuhao[]" id="gongzuo_xuhao"  value="" onblur="tableDate();" style="width:100%"> </td>';
        gongzuo_html += '<td><input type="text" name="gongzuo_qishishijian[]" id="gongzuo_qishishijian"  class="form-control input-date" value="" onblur="tableDate();" style="width:100%"> </td>';
        gongzuo_html += '<td><input type="text" name="gongzuo_jieshushijian[]" id="gongzuo_jieshushijian"  class="form-control input-date" value="" onblur="tableDate();" style="width:100%"> </td>';

        gongzuo_html += '<td><input type="text" name="gongzuo_danwei[]" id="gongzuo_danwei"  value="" onblur="tableDate();" style="width:100%"> </td>';
        gongzuo_html += '<td><input type="text" name="gongzuo_bumeng[]" id="gongzuo_bumeng"  value="" onblur="tableDate();" style="width:100%"></td>';
        gongzuo_html += '<td><input type="text" name="gongzuo_zhiwu[]" id="gongzuo_zhiwu"  value="" onblur="tableDate();" style="width:100%"></td>';

        gongzuo_html += '<td><input type="text" name="gongzuo_beizhu[]" id="gongzuo_beizhu"  value="" onblur="tableDate();" style="width:100%"></td>';

        gongzuo_html += '<td><a href="javascript:void(0);" onclick="delete_gongzuos(this)" class="delrow">删除</a></td>';
        gongzuo_html += '</tr>';

        $('#gongzuos').before(gongzuo_html);

        $(document).ready(function() {

            $('.input-date').datetimepicker({
                format : 'yyyy-mm-dd',
                minView: "month",
                locale : 'zh-cn',
                //locale: moment.locale('zh-cn'),
                ignoreReadonly : true,
                autoclose: 1,
                widgetPositioning : {
                    horizontal : 'auto',
                    vertical : 'bottom'
                },
            });

            $(".input-daterange input").datetimepicker({
                format : "yyyy-mm-dd",
                locale : 'zh-cn',
                minView: "month",
                showTodayButton : true,
                showClose : true,
                ignoreReadonly : true,
                widgetPositioning : {
                    horizontal : 'auto',
                    vertical : 'bottom'
                },
            });

            $("#start_date").on("dp.change", function(e) {
                if ($("#end_date").length > 0) {
                    $('#end_date').data("DateTimePicker").minDate(e.date);
                }
            });

            $("#end_date").on("dp.change", function(e) {
                if ($("#start_date").length > 0) {
                    $('#start_date').data("DateTimePicker").maxDate(e.date);
                }
            });

            $(".input-date-time").datetimepicker({
                format : 'yyyy-mm-dd',
                locale : 'zh',
                minView: "month",
                sideBySide : true,
                autoclose: 1,
                showTodayButton : true,
                showClose : true,
                ignoreReadonly : true,
                widgetPositioning : {
                    horizontal : 'auto',
                    vertical : 'bottom'
                },
            });

        });

    }

    //删除一行
    function delete_gongzuos(obj)
    {
        $(obj).parent().parent().remove();//删除一个物品对象
        tableDate();
    }



</script>



