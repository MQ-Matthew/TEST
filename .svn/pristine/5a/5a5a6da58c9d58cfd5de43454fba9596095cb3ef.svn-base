                ignoreReadonly : true,
                autoclose: 1,
                widgetPositioning : {
                    horizontal : 'auto',
                    vertical : 'bottom'
                },
            });

            $(".input-daterange input").datetimepicker({
                format : "yyyy-mm-dd",
                locale : 'zh-cn',
                minView: "month",
                showTodayButton : true,
                showClose : true,
                ignoreReadonly : true,
                widgetPositioning : {
                    horizontal : 'auto',
                    vertical : 'bottom'
                },
            });

            $("#start_date").on("dp.change", function(e) {
                if ($("#end_date").length > 0) {
                    $('#end_date').data("DateTimePicker").minDate(e.date);
                }
            });

            $("#end_date").on("dp.change", function(e) {
                if ($("#start_date").length > 0) {
                    $('#start_date').data("DateTimePicker").maxDate(e.date);
                }
            });

            $(".input-date-time").datetimepicker({
                format : 'yyyy-mm-dd',
                locale : 'zh',
                minView: "month",
                sideBySide : true,
                autoclose: 1,
                showTodayButton : true,
                showClose : true,
                ignoreReadonly : true,
                widgetPositioning : {
                    horizontal : 'auto',
                    vertical : 'bottom'
                },
            });

        });

	


    </script>




