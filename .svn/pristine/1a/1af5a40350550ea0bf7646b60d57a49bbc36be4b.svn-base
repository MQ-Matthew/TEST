<extend name="Layout/ins_page" />
<block name="content">


    <style>
        .backgroundcolor{
            background-color: #F5F5F5;
        }
        table tr:hover{background-color: #CCCCCC ;}
        .page-header{
            border-bottom: 0px solid #eee;
			
        }
        .pull-left{
            margin-bottom: -20px;
        }
        .pull-right{
            margin-bottom: -20px;
        }
		<--.panel-body{
			width:1106px;
			height:200px;
			//float:right;
		}-->
		.col-sm-4{
		//float:right;
		}
    </style>
	<select name="dept_list" id="dept_list" class="hidden">
        {:fill_option($dept_list)}
    </select>
    <select name="agent_list" id="agent_list" class="hidden">
     {:fill_option($agent_list)}
    </select>
    <div class="operate panel panel-default">
        <div class="panel-body">
            <div class="pull-left">
                {:W('PageHeader/asset_simple',array('name'=>get_dept_name($dept_id).'(部门)  /  '.get_assettype($type).'(资产类型)'))}
				
            </div>
			<div class="col-sm-4 search_box">
		<form name="form_search" id="form_search" method="post" class="pull-right">
			<div class="input-group col-20">
				<input  class="form-control" type="text" name="keyword" id="keyword"/>
				<div class="input-group-btn">
					<a class="btn btn-sm btn-info" onclick="submit_search();"><i class="fa fa-search" ></i></a>
					<a class="btn btn-sm btn-success" onclick="toggle_adv_search();"><i id="toggle_adv_search_icon" class="fa fa-chevron-down bigger-125"></i></a>
				</div>
			</div>
		</form>
	</div>
			 
           <div class="pull-right">
				
                <a onclick="javascript:window.history.back(-1);" class="btn btn-sm btn-primary">返回</a>
                <a  onclick="regres()"  class="btn btn-sm btn-danger">退回</a>
            </div>
        </div>
    </div>
	<form method="post" name="form_adv_search" id="form_adv_search">
        <div class="adv_search panel panel-default hidden" id="adv_search">
            <div class="panel-heading">
                <div class="row">
                    <h4 class="col-xs-6">高级搜索</h4>
                    <div class="col-xs-6 text-right">
                        <a  class="btn btn-sm btn-info" onclick="submit_adv_search();">搜索</a>
                        <a  class="btn btn-sm" onclick="close_adv_search();">关闭</a>
                    </div>
                </div>
            </div>
            <div class="panel-body">


                <div class="form-group col-sm-6">
                    <label class="col-sm-4 control-label" for="eq_dept_id">部门：</label>
                    <div class="col-sm-8">
                        <select class="form-control" name="eq_dept_id" id="eq_dept_id">
                            <option value="">全部</option>
                            {:fill_option($dept_list)}
                        </select>
                    </div>
                </div>

                <div class="form-group col-sm-6">
                    <label class="col-sm-4 control-label" for="eq_position_id">职位：</label>
                    <div class="col-sm-8">
                        <select class="form-control" name="eq_position_id" id="eq_position_id">
                            <option value="">全部</option>
                            {:fill_option($position_list)}
                        </select>

                    </div>
                </div>
            </div>
        </div>
    </form>

    <div class="row">
        <div class="col-sm-12 sub_left_menu ">
            <form id="form_user" name="form_data" method="post" class="form-horizontal">
                <table class="table table-bordered">
                    <thead>
                        <tr  style="background-color:#000000;text-align: center;">
                            <th class="text-center col-4"><label class="inline pull-left">
                                <input class="ace" type="checkbox" name="id-toggle-all" id="id-toggle-all" />
                                <span class="lbl"></span></label></th>
                            <th>id</th>
                            <th>部门</th>
                            <th>资产类型</th>
                            <th>设备码</th>
                            <th>资产名称</th>
                            <th>品牌</th>
                            <th>数量</th>

                            <th>领用时间</th>
                            <th>使用人</th>
                            <th>操作</th>
                        </tr>
                    </thead>

                    <tbody>

                    <empty name="list">
                        <tr class="tbody">
                            <td colspan="8">暂无数据</td>
                        </tr>
                    <else/>

                        <volist name="list" id="data">
                            <tr  id="{$data.id}" class="tbody">
                                <td class="text-center col-3"><label class="inline pull-left">
                                    <input class="ace" type="checkbox" name="id[]" value="{$data.id}" />
                                    <span class="lbl"></span></label></td>
                                <td class="">{$data.id}</td>
                                <td class="">{:get_dept_name($data['receive_deptid'])}</td>
                                <td class="">{:get_assettype($data['type'])}</td>
                                <td class="">{$data.code}</td>
                                <td class="">{$data.name}</td>
                                <td class="">{$data.brand}</td>
                                <td class="">{$data.amount}</td>

                                <td class="">{$data.receive_time|date="Y-m-d",###}</td>
                                <td class=""><a href="{:U('Asset/look',array('id'=>$data['id']))}">{$data.receive_userid|get_assetuser_name}</a></td>
                                <td class="">
                                   <a href="{:U('Asset/guiku',array('id'=>$data['id']))}">归库</a>&nbsp;&nbsp;|&nbsp;&nbsp;
                                    <a  href="{:U('Asset/service',array('id'=>$data['id']))}">维修</a>&nbsp;&nbsp;|&nbsp;&nbsp;
									<a  href="{:U('Asset/bianji',array('id'=>$data['id']))}">编辑</a>
                                </td>
                            </tr>
                      </volist>
                    </empty>
                    </tbody>
                </table>

            </form>
            <div class="pagination">
                {$page}
            </div>
        </div>

    </div>


</block>

<block name="js">

    <script type="text/javascript">

        function regress(id){

            var id = $(id).attr('share_id');

            ///alert(id);return;
            var vars = $("#form_user").serialize();
          ///  alert(vars);return;

            ui_confirm('确定要删除吗?', function() {
                sendAjax("{:U('regress')}", vars, function(data) {
                    /*if (data.status) {
                        ui_alert(data.info, function() {
                            location.reload(true);
                        });
                    }*/
                });
            });
        }


        function regres(){

            if ($("input[name='id[]']:checked").length == 0) {
                ui_error('请选择要退回的资产!');
                return;
            }
            var vars = $("#form_user").serialize();
           // alert(vars);return;

            ui_confirm('确定要退回所选的资产吗?', function() {
                sendAjax("{:U('regress')}", vars, function(data) {
                    /*if (data.status) {
                     ui_alert(data.info, function() {
                     location.reload(true);
                     });
                     }*/
                });
            });
        }
    </script>

</block>