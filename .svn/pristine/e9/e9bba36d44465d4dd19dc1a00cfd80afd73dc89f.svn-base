<!--全日制教育经历 table -->
<table cellspacing="0" class="table table-bordered table-condensed" style="width: 100%;">
    <thead>
    <th colspan="8">全日制教育经历 ：</th>
    <tr>
        <th width="12%">入学时间</th>
        <th width="12%">毕业时间</th>
        <th width="20%">学校名称</th>
        <th width="20%">专业</th>
        <th width="8%">学制</th>
        <th width="10%">学历</th>
        <th width="8%">学位</th>
        <th width="10%">操作</th>
    </tr>
    </thead>

    <empty name="info.jiaoyu">
        <tbody id='all_goods'>  <!--添加-->
            <tr>
            <td><input type="text" name="jy_rxsj[]" id="jy_rxsj"  class="form-control input-date" value="" onblur="tableDate();" style="width:100%"> </td>
            <td><input type="text" name="jy_bysj[]" id="jy_bysj"  class="form-control input-date" value="" onblur="tableDate();" style="width:100%"> </td>
            <td><input type="text" name="jy_xxmc[]" id="jy_xxmc"  value="" onblur="tableDate();" style="width:100%"> </td>
            <td>
                <input type="text" name="jy_zy[]" id="jy_zy"  value="" onblur="tableDate();" style="width:100%">
            </td>
            <td>
                <input type="text" name="jy_xz[]" id="jy_xz"  value="" onblur="tableDate();" style="width:100%">
            </td>
            <td>
                <select class="form-control" id="jy_xl"  onchange="tableDate();" >
                    <option value=''>请选择</option>
                    <option value='大专'>大专</option>
                    <option value='本科'>本科</option>
                    <option value='硕士'>硕士</option>
                    <option value='博士'>博士</option>
                    <option value='其它'>其它</option>
                </select>

            </td>
            <td>
                <input type="text" name="jy_xw[]" id="jy_xw"  value="" onblur="tableDate();" style="width:100%">
            </td>

            <td><a href="javascript:void(0);" onclick="delete_goods(this)" class="delrow">删除</a></td>
        </tr>
            <tr id='goods'></tr>
        </tbody>


     <else />

        <tbody id='all_goods'> <!--编辑-->
            <tr id='goods'></tr>
        </tbody>

    </empty>

    <tr>
        <td><button class="btn btn-small btn-success" type="button" onclick="tbAddRow()">添加全日制教育经历</button></td>
        <td colspan="10"></td>
    </tr>

</table>
<input type="hidden" id="jiaoyu" name="jiaoyu" value="{$info.jiaoyu}">


